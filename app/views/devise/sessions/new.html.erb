

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

<div class="container">
    <div class="col-md-6 col-md-offset-3">
      <form class="form-signin">
        <h2 class="form-signin-heading">Please sign in</h2>
        <label for="inputEmail" class="sr-only">Email address</label>
        <%= f.email_field :email, autofocus: true, class:"form-control", placeholder:"Enter your email address" %><br>
        <label for="inputPassword" class="sr-only">Password</label>
        <%= f.password_field :password, autocomplete: "off", class:"form-control", placeholder:"Enter your password" %>
        <div class="checkbox">
            <% if devise_mapping.rememberable? -%>
              <%= f.check_box :remember_me %>
              <%= f.label :remember_me %>
          <% end %>
        </div>
        <%= f.submit "Log in", class:"btn btn-lg btn-primary btn-block" %>
       
        
      </form>
     
      <%= render "devise/shared/links" %>
   </div>
  </div> <!-- /container -->

  

<% end %>
<br><br><br><br><br><br><br><br>
